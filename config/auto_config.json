{
  "min_len": 6,
  "min_freq": 3,
  "max_entries": 256,
  "min_gain": 10,
  "alias_style": "ns_base36",
  "namespaces": { "scope": true, "key": true, "value": true, "text": true },
  "ignore_blocks": true,
  "strict_no_alias_equals_source_token": true,
  "stoplist": ["the","a","an","and","or","to","of","in","for","on","with","as","by","at","from"],
  "protect": {
    "negations": ["no","not","never"],
    "modals": ["must","should","may"]
  }
}